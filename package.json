{
  "name": "react-boilerplate",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "tsc --noEmit && vite build",
    "dev": "vite --host 0.0.0.0 --port 3000",
    "postinstall": "fenge install",
    "style": "fenge",
    "style:update": "fenge -u"
  },
  "dependencies": {
    "react": "19.0.0"
  },
  "devDependencies": {
    "@tailwindcss/vite": "4.0.9",
    "@types/react": "19.0.10",
    "@types/web": "0.0.208",
    "@vitejs/plugin-react": "4.3.4",
    "daisyui": "5.0.0",
    "fenge": "0.7.2",
    "react-dom": "19.0.0",
    "tailwindcss": "4.0.9",
    "typescript": "5.8.3",
    "vite": "6.2.0"
  },
  "engines": {
    "node": ">=18"
  },
  "devEngines": {
    "runtime": {
      "name": "node",
      "version": ">=18"
    },
    "packageManager": {
      "name": "pnpm",
      "version": "10.7.1"
    }
  }
}
