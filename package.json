{
  "name": "react-boilerplate",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "tsc --noEmit && vite build",
    "dev": "vite --host 0.0.0.0 --port 3000",
    "postinstall": "fenge install",
    "style": "fenge",
    "style:update": "fenge -u"
  },
  "dependencies": {
    "react": "19.1.1",
    "react-dom": "19.1.1"
  },
  "devDependencies": {
    "@tailwindcss/vite": "4.1.11",
    "@types/react": "19.1.9",
    "@types/react-dom": "19.1.7",
    "@types/web": "0.0.260",
    "@vitejs/plugin-react": "4.7.0",
    "daisyui": "5.0.50",
    "fenge": "0.9.9",
    "tailwindcss": "4.1.11",
    "typescript": "5.8.3",
    "vite": "7.0.6"
  },
  "engines": {
    "node": ">=20"
  },
  "devEngines": {
    "runtime": {
      "name": "node",
      "version": ">=20"
    },
    "packageManager": {
      "name": "pnpm",
      "version": "10.12.4"
    }
  }
}
